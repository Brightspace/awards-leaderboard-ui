{
  "name": "awards-leaderboard-ui",
  "appId": "urn:d2l:fra:class:awards-leaderboard-ui",
  "description": "Awards Leaderboard UI",
  "version": "2.0.2",
  "scripts": {
    "start:prod": "npm run build && npx http-server ./build/prod -p 3000",
    "start:dev": "npm run build && npx polymer serve -p 3000",
    "build": "polymer build && npm run build:appconfig",
    "build:appconfig": "gulp build-appconfig",
    "clean": "gulp clean",
    "rebuild": "npm run clean && npm run build",
    "lint": "eslint --ext .json,.js,.html .",
    "lint:fix": "eslint --ext .json,.js,.html --fix .",
    "test": "npm run lint && npm run test:local",
    "test:local": "polymer test --skip-plugin sauce",
    "test:sauce": "polymer test --skip-plugin local",
    "test:license": "license-checker-ci",
    "test:travis": "polymer test --config-file wct.conf-travis.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Brightspace/awards-leaderboard-ui.git"
  },
  "author": "D2L",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Brightspace/awards-leaderboard-ui/issues"
  },
  "homepage": "https://github.com/Brightspace/awards-leaderboard-ui#readme",
  "config": {
    "frauAppConfigBuilder": {
      "appFile": "index.html",
      "distLocal": "./build/dev",
      "dist": "./build/prod",
      "envVar": "TRAVIS",
      "loader": "iframe",
      "endpoint": "http://localhost:3000/"
    }
  },
  "dependencies": {
    "@brightspace-ui/core": "^1.28.3",
    "@bundled-es-modules/uuid": "^3.3.2",
    "@material/textfield": "^0.40.0",
    "@polymer/paper-dialog": "^3.0.1",
    "@polymer/polymer": "^3.3.1",
    "@webcomponents/webcomponentsjs": "^2.4.1",
    "d2l-fetch": "github:Brightspace/d2l-fetch#semver:^2",
    "d2l-fetch-auth": "^1.2.0",
    "d2l-html-editor": "github:Brightspace/d2l-html-editor",
    "d2l-inputs": "github:BrightspaceUI/inputs#semver:^2",
    "d2l-loading-spinner": "github:BrightspaceUI/loading-spinner#semver:^7",
    "d2l-resize-aware": "github:BrightspaceUI/resize-aware#semver:^1",
    "d2l-tooltip": "github:BrightspaceUI/tooltip#semver:^3",
    "frau-locale-provider": "^1.2.0",
    "lit-element": "^2.2.1",
    "lit-html": "^1.1.2",
    "pwa-helpers": "^0.9.1",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "reselect": "^3.0.1"
  },
  "devDependencies": {
    "@brightspace-ui/visual-diff": "^1.1.1",
    "@polymer/test-fixture": "^4.0.2",
    "axe-core": "^3.4.1",
    "babel-eslint": "^10.0.3",
    "d2l-license-checker": "^3.0.0",
    "del": "^3.0.0",
    "eslint": "^5.16.0",
    "eslint-config-brightspace": "^0.5.3",
    "eslint-plugin-chai-friendly": "^0.5.0",
    "eslint-plugin-html": "^5.0.3",
    "eslint-plugin-json": "^2.0.1",
    "eslint-plugin-lit": "^0.6.0",
    "eslint-plugin-mocha": "^6.2.2",
    "gulp": "^4.0.2",
    "gulp-string-replace": "^1.1.2",
    "http-server": "^0.11.1",
    "mocha": "^7.0.0",
    "polymer-cli": "^1.9.11",
    "polyserve": "^0.27.15",
    "puppeteer": "^2.0.0",
    "vinyl-buffer": "^1.0.1",
    "vinyl-source-stream": "^2.0.0",
    "wct-mocha": "^1.0.1",
    "web-component-tester": "^6.9.2"
  }
}
